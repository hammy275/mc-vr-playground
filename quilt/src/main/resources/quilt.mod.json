    "version": "${version}",
    "name": "MC VR Playground",
    "description": "Demo mod for Vivecraft API",
    "authors": ["hammy275"],
    "contact": {
      "sources": "https://github.com/architectury/architectury-templates"
    },
    "license": "MIT License",
    "icon": "assets/mc_vr_playground/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
